{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [".\\Examples\\helloworld.lis"],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "f:/LisCompiler/",
      "program": "f:/LisCompiler/Build/Binaries/Compiler.exe",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}